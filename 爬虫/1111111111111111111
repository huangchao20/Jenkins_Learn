<script type="text/javascript">
    var isLogined= false;
    var pageUrl = location.href;
    var pageSize = 30;
    var isPostingComment = false;
    var currentTag = $("#tagId").val();
   

    Pager.PageSize = 30;
    Pager.ShowPageCount = 4;
    Pager.ClickFunctionName = 'GetIngList';
    $(function () {
        isWeb = true;
        webIndexLoad();
    });

    function webIndexLoad() {
        var pageIndex = 1;
        var hashString = '#' + currentIngListType;
        var hashPageString = '#' + currentIngListType + '/p';
        if (currentTag != "") {
            currentIngListType = 'tag';
            if (location.hash.indexOf('#p') == 0) {
                pageIndex = parseInt(location.hash.substr(2));
            }
        }
        else {
            if (location.hash.indexOf('#') == 0) {
                if (location.hash.indexOf('/p') > 0) {
                    pageIndex = parseInt(location.hash.substr(location.hash.indexOf('/p') + 2));
                    currentIngListType = location.hash.substr(1, location.hash.indexOf('/p') - 1);
                }
                else {
                    currentIngListType = location.hash.substr(1);
                }
            } else {
                currentIngListType = "all";
            }
        }
        SetIngTab(currentIngListType, pageIndex);
        if (isLogined) {
            setInterval(AutoRefreshIng, 120000);
        }
        //set group li change
        $('.group_list').on('click', 'li>a', function () {
            $('.group_list li.active').removeClass('active');
            $(this).parent().addClass('active');
        })
    }
</script>
</div>
