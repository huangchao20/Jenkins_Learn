    function search_answer() {
        window.location = 'http://zzk.cnblogs.com/s?t=q&w=' + encodeURIComponent($("#souword").val());
    }
    function search_answer_enter(event) {
        if (event.keyCode == 13) {
            search_answer();
            return false;
        }
        else {
            return true;
        }
